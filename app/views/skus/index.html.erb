<p id="notice"><%= notice %></p>

<h1>Skus</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skus.each do |sku| %>
      <tr>
        <td><%= sku.sku %></td>
        <td><%= sku.supplier_id %></td>
        <td><%= sku.part_name%></td>
        <td><%= sku.col_1%></td>
        <td><%= sku.col_2%></td>
        <td><%= sku.col_3%></td>
        <td><%= sku.col_4%></td>
        <td><%= sku.col_5%></td>
        <td><%= sku.price%></td>
        <td><%= link_to 'Show', sku %></td>
        <td><%= link_to 'Edit', edit_sku_path(sku) %></td>
        <td><%= link_to 'Destroy', sku, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Sku', new_sku_path %>
